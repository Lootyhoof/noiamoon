<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>noia-options@addons.palemoon.org</em:id>
    <em:version>2.1.0</em:version>
    <em:name>Noia Moon Options</em:name>
    <em:description>Options manager for Noia Moon theme.</em:description>
    <em:creator>Pale Moon Add-Ons Team</em:creator>
    <em:type>2</em:type>
    <em:optionsURL>chrome://noiamoonoptions/content/options.xul</em:optionsURL>

    <em:contributor>David Vincent</em:contributor>

    <em:targetApplication>
      <!-- Pale Moon -->
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>28.6.0</em:minVersion>
        <em:maxVersion>30.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:targetApplication>
      <!-- Applications using Firefox GUID -->
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <!--
            Pale Moon 30 and above will stop using its own GUID to
            "further improve extension compatibility"
          -->
        <em:minVersion>30.0.0a1</em:minVersion>
        <em:maxVersion>30.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
